#!/usr/bin/env bash

echo "== send"

root_path=$(realpath .)
message_attributes_path="file://${root_path}/send-message.json"

QUEUE_URL=https://sqs.us-east-1.amazonaws.com/976193260990/sqs-standard-MyQueue-Dupg0LMQXuA5

aws sqs send-message \
--queue-url $QUEUE_URL \
--message-body "Hello Mars" \
--delay-seconds 0 \
--message-attributes $message_attributes_path